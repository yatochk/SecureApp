<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_gallery">

    <fragment
        android:id="@+id/navigation_gallery"
        android:name="com.yatochk.secure.app.ui.home.HomeFragment"
        android:label="@string/title_gallery"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_contact"
        android:name="com.yatochk.secure.app.ui.dashboard.DashboardFragment"
        android:label="@string/title_contacts"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_internet"
        android:name="com.yatochk.secure.app.ui.notifications.NotificationsFragment"
        android:label="@string/title_notes"
        tools:layout="@layout/fragment_notifications" />
</navigation>